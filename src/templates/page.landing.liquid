    <div class="product-info">
      <a href="{{ uni.url }}" class="product-info__img-link">
        <img
        src="{{ uni.featured_image | img_url:'100x' }}"
        alt=""
        class="lazyload blur product-info__img"
        data-sizes="auto"
          data-src="{{ uni.featured_image | product_img_url: '900x' }}"
          data-srcset=" {{ uni.featured_image | product_img_url: '300x' }} 300w,
                        {{ uni.featured_image | product_img_url: '400x' }} 400w,
                        {{ uni.featured_image | product_img_url: '500x' }} 500w,
                        {{ uni.featured_image | product_img_url: '600x' }} 600w,
                        {{ uni.featured_image | product_img_url: '700x' }} 700w,
                        {{ uni.featured_image | product_img_url: '800x' }} 800w,
                        {{ uni.featured_image | product_img_url: '900x' }} 900w"
        />
      </a>

      <h2 class="product-info__title"><a href="{{ uni.url }}" class="product-info__title-link">{{ uni.title }}</a></h2>

      {% if plum.metafields.custom_fields["product_subtitle"] != blank %}
			    <p class="product-info__subtitle">{{ plum.metafields.custom_fields["product_subtitle"] }}</p>
      {% endif %}

      <h4 class="product-info__price">{{ uni.price | money }}</h4>

      <form action="/cart/add" class="product-info__form">
        <input type="hidden" name="id" value="{{ uni.variants.first.id }}">
        <button type="submit" name="add" class="action_button add_to_cart product-info__add" data-label={{ add_to_cart_label | json }}>
          {{ 'products.product.add_to_cart' | t }}
        </button>
      </form>

    </div>
  </div>

